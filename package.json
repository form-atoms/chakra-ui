{
  "name": "@form-atoms/chakra-ui",
  "version": "0.0.0-semantic-release",
  "type": "module",
  "author": "Miroslav Petrik <miroslavpetrik@outlook.com>",
  "homepage": "https://github.com/MiroslavPetrik/form-atoms-chakra-ui#readme",
  "repository": "github:MiroslavPetrik/form-atoms-chakra-ui",
  "bugs": "https://github.com/MiroslavPetrik/form-atoms-chakra-ui/issues",
  "license": "MIT",
  "scripts": {
    "build": "tsc --build ./tsconfig.build.json",
    "storybook": "storybook dev -p 6007",
    "sb": "yarn storybook",
    "build:storybook": "storybook build",
    "lint": "eslint --ext .tsx,.ts ./src",
    "lint:fix": "yarn run lint --fix",
    "ui": "yarn upgrade-interactive",
    "semantic-release": "semantic-release"
  },
  "packageManager": "yarn@4.0.2",
  "keywords": [
    "react atom form",
    "react field",
    "react input",
    "chakra ui react",
    "chakra ui form field",
    "form atoms",
    "jotai",
    "jotai form",
    "react",
    "react form",
    "react hook"
  ],
  "devDependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "2.8.2",
    "@chakra-ui/storybook-addon": "5.1.0",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@form-atoms/field": "^3.8.0",
    "@mdx-js/react": "^2.3.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.4",
    "@semantic-release/npm": "^11.0.1",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@storybook/addon-actions": "7.6.2",
    "@storybook/addon-docs": "7.6.2",
    "@storybook/addon-essentials": "7.6.2",
    "@storybook/addon-interactions": "7.6.2",
    "@storybook/addon-links": "7.6.2",
    "@storybook/builder-vite": "^7.6.2",
    "@storybook/jest": "0.2.3",
    "@storybook/manager-api": "^7.6.2",
    "@storybook/react": "7.6.2",
    "@storybook/react-vite": "^7.6.2",
    "@storybook/testing-library": "0.2.2",
    "@storybook/theming": "7.6.2",
    "@testing-library/jest-dom": "6.1.5",
    "@testing-library/react": "14.1.2",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.5.1",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/semantic-release": "^20.0.6",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "eslint": "8.54.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "5.0.1",
    "form-atoms": "^3.2.0",
    "framer-motion": "8.5.4",
    "jotai": "2.6.0",
    "jotai-devtools": "^0.7.0",
    "jotai-effect": "^0.2.3",
    "prettier": "^3.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-render-prop-type": "0.1.0",
    "semantic-release": "^22.0.8",
    "storybook": "^7.6.2",
    "typescript": "^5.3.2",
    "vite": "^5.0.4",
    "vite-tsconfig-paths": "^4.2.1",
    "zod": "3.22.4"
  },
  "peerDependencies": {
    "@chakra-ui/react": "2.8.2",
    "@form-atoms/field": "^3.6.0"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "next",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version}\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
